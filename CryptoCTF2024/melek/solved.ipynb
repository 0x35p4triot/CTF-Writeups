   "source": [
    "from Crypto.Util.number import long_to_bytes\n",
    "from output import enc\n",
    "\n",
    "e, p, PT = enc\n",
    "\n",
    "F = GF(p)\n",
    "R = F['x']\n",
    "\n",
    "poly = R.lagrange_polynomial(PT)\n",
    "ct = poly.coefficients()[0]\n",
    "m = (ct^(Zmod(p - 1)(e // 2)^-1)).sqrt()\n",
    "\n",
    "print(long_to_bytes(int(m)))"
   ]
  }
